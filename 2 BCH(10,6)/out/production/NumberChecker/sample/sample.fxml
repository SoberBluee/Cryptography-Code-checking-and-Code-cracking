<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="bg_panel" prefHeight="250" prefWidth="398.0">
         <children>
            <Label layoutX="14.0" layoutY="15.0" text="6 Digit Number" />
            <TextField fx:id="input" layoutX="104.0" layoutY="11.0" prefHeight="25.0" prefWidth="194.0" />
            <Button fx:id="check" layoutX="303.0" layoutY="11.0" mnemonicParsing="false" onAction="#check_number" prefHeight="25.0" prefWidth="82.0" text="Check" />
            <Label layoutX="14.0" layoutY="46.0" text="BCH Decode" />
            <TextField fx:id="decode_input" layoutX="104.0" layoutY="42.0" prefHeight="25.0" prefWidth="194.0" />
            <Button fx:id="check_decode" layoutX="303.0" layoutY="42.0" mnemonicParsing="false" onAction="#decode" prefHeight="25.0" prefWidth="82.0" text="Decode" />
            <TextArea fx:id="output" layoutX="11.0" layoutY="75.0" prefHeight="116.0" prefWidth="377.0" />
         </children>
      </Pane>
   </children>
</GridPane>
